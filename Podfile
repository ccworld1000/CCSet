# Uncomment the next line to define a global platform for your project

target 'CCSet iOS' do
  platform :ios, '9.0'
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CCSet iOS
  pod 'CCSet'

end

target 'CCSet macOS' do
  platform :osx, '10.9'
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CCSet macOS
  pod 'CCSet'

end
